    <key>repository</key>
    <dict>
        <key>comments</key>
        <dict>
            <key>patterns</key>
            <array>
                <dict>
                    <key>name</key>
                    <string>comment.line.double-slash.pseudocode</string>
                    <key>match</key>
                    <string>//.*$</string>
                </dict>
                <dict>
                    <key>name</key>
                    <string>comment.block.pseudocode</string>
                    <key>begin</key>
                    <string>/\*</string>
                    <key>end</key>
                    <string>\*/</string>
                </dict>
            </array>
        </dict>
        <key>strings</key>
        <dict>
            <key>patterns</key>
            <array>
                <dict>
                    <key>name</key>
                    <string>string.quoted.double.pseudocode</string>
                    <key>begin</key>
                    <string>"</string>
                    <key>end</key>
                    <string>"</string>
                    <key>patterns</key>
                    <array>
                        <dict>
                            <key>name</key>
                            <string>constant.character.escape.pseudocode</string>
                            <key>match</key>
                            <string>\\.</string>
                        </dict>
                    </array>
                </dict>
                <dict>
                    <key>name</key>
                    <string>string.quoted.single.pseudocode</string>
                    <key>begin</key>
                    <string>'</string>
                    <key>end</key>
                    <string>'</string>
                    <key>patterns</key>
                    <array>
                        <dict>
                            <key>name</key>
                            <string>constant.character.escape.pseudocode</string>
                            <key>match</key>
                            <string>\\.</string>
                        </dict>
                    </array>
                </dict>
            </array>
        </dict>
        <key>numbers</key>
        <dict>
            <key>patterns</key>
            <array>
                <dict>
                    <key>name</key>
                    <string>constant.numeric.hex.pseudocode</string>
                    <key>match</key>
                    <string>\b0[xX][0-9a-fA-F]+\b</string>
                </dict>
                <dict>
                    <key>name</key>
                    <string>constant.numeric.binary.pseudocode</string>
                    <key>match</key>
                    <string>\b0[bB][01]+\b</string>
                </dict>
                <dict>
                    <key>name</key>
                    <string>constant.numeric.float.pseudocode</string>
                    <key>match</key>
                    <string>\b[0-9]+\.[0-9]+([eE][+-]?[0-9]+)?\b</string>
                </dict>
                <dict>
                    <key>name</key>
                    <string>constant.numeric.integer.pseudocode</string>
                    <key>match</key>
                    <string>\b[0-9]+\b</string>
                </dict>
            </array>
        </dict>
        <key>keywords</key>
        <dict>
            <key>patterns</key>
            <array>
                <dict>
                    <key>name</key>
                    <string>keyword.control.pseudocode</string>
                    <key>match</key>
                    <string>\b(if|elif|else|then|while|for|in|to|do|return|break|continue|match|case|end|_)\b</string>
                </dict>
                <dict>
                    <key>name</key>
                    <string>keyword.declaration.pseudocode</string>
