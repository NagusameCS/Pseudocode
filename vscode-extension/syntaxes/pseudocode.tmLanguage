                    <key>name</key>
                    <string>keyword.operator.logical.pseudocode</string>
                    <key>match</key>
                    <string>\b(and|or|not)\b</string>
                </dict>
            </array>
        </dict>
        <key>functions</key>
        <dict>
            <key>patterns</key>
            <array>
                <dict>
                    <key>name</key>
                    <string>entity.name.function.pseudocode</string>
                    <key>match</key>
                    <string>\b([a-zA-Z_][a-zA-Z0-9_]*)\s*(?=\()</string>
                </dict>
                <dict>
                    <key>match</key>
                    <string>\b(fn)\s+([a-zA-Z_][a-zA-Z0-9_]*)</string>
                    <key>captures</key>
                    <dict>
                        <key>1</key>
                        <dict>
                            <key>name</key>
                            <string>keyword.declaration.pseudocode</string>
                        </dict>
                        <key>2</key>
                        <dict>
                            <key>name</key>
                            <string>entity.name.function.definition.pseudocode</string>
                        </dict>
                    </dict>
                </dict>
            </array>
        </dict>
        <key>builtins</key>
        <dict>
            <key>patterns</key>
            <array>
                <dict>
                    <key>name</key>
                    <string>support.function.builtin.pseudocode</string>
                    <key>match</key>
                    <string>\b(print|println|input|len|str|int|float|type|push|pop|slice|concat|split|join|replace|upper|lower|trim|substr|index_of|contains|starts_with|ends_with|char_at|ord|chr|keys|values|has|has_key|sort|reverse|range|abs|min|max|floor|ceil|round|sqrt|pow|random|clock|bit_and|bit_or|bit_xor|bit_not|bit_lshift|bit_rshift|popcount|clz|ctz|read_file|write_file|append_file|file_exists|delete_file|list_dir|mkdir|http_get|http_post|http_put|http_delete|json_parse|json_stringify|exec|env|set_env|args|exit|sleep|dict|dict_get|dict_set|dict_has|dict_keys|dict_values|dict_delete|sin|cos|tan|asin|acos|atan|atan2|log|log10|log2|exp|hypot|sha256|md5|encode_base64|decode_base64|map|filter|reduce|find)\b</string>
                </dict>
            </array>
        </dict>
        <key>operators</key>
        <dict>
            <key>patterns</key>
            <array>
                <dict>
                    <key>name</key>
                    <string>keyword.operator.comparison.pseudocode</string>
                    <key>match</key>
                    <string>(==|!=|&lt;=|&gt;=|&lt;|&gt;)</string>
                </dict>
                <dict>
                    <key>name</key>
                    <string>keyword.operator.assignment.pseudocode</string>
                    <key>match</key>
                    <string>(=|\+=|-=|\*=|/=)</string>
                </dict>
                <dict>
                    <key>name</key>
                    <string>keyword.operator.arithmetic.pseudocode</string>
                    <key>match</key>
                    <string>(\+|-|\*|/|%|\^)</string>
                </dict>
                <dict>
                    <key>name</key>
                    <string>keyword.operator.range.pseudocode</string>
                    <key>match</key>
                    <string>\.\.</string>
                </dict>
            </array>
        </dict>
        <key>variables</key>
        <dict>
            <key>patterns</key>
            <array>
                <dict>
                    <key>name</key>
                    <string>variable.other.pseudocode</string>
                    <key>match</key>
                    <string>\b[a-zA-Z_][a-zA-Z0-9_]*\b</string>
                </dict>
            </array>
        </dict>
    </dict>
</dict>
</plist>
