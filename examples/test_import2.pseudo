// Test with import - define pred first
fn is_big(x)
    print("is_big called with: " + str(x))
    let result = x > 3
    print("returning: " + str(result))
    return result
end

import "./lib/functional.pseudo"

let nums = [1, 2, 3, 4, 5]
print("calling find...")
let found = find(nums, is_big)
print("found=" + str(found))
